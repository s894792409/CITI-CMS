<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />

    <title>@ViewBag.Title - CMS</title>
    <link rel="stylesheet" href="~/css/bundlecss.min.css" />
</head>
<body>
    <div id="wrapper" class="container-fluid"style="flex-direction:row;display: flex; flex-wrap:wrap;">
        <div class="row"  >
            @await Component.InvokeAsync(nameof(UserDetail))
            <section id="show" class="col-sm-9">@RenderBody()</section>
            <footer id="mainFooter" class="col-sm-12">&copy; @DateTime.Now.Year - CITI CMS</footer>
        </div>
    </div>
    <script src="~/js/bundlejs.min.js"></script>
    @RenderSection("Script", false)
    
</body>
</html>
