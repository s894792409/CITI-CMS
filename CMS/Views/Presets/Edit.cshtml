@model CMS.Models.ViewModels.Preset

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>Preset</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="presetId" />
            <div class="form-group">
                <label asp-for="themeId" class="control-label"></label>
                <input asp-for="themeId" class="form-control" />
                <span asp-validation-for="themeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="visitId" class="control-label"></label>
                <select asp-for="visitId" class="form-control" asp-items="@ViewBag.selectlist "></select>
                <span asp-validation-for="visitId" class="text-danger"></span>
            </div>
            <input asp-for="dateCreated" class="form-control" style="display: none;" />
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
