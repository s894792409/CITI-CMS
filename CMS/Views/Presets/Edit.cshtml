@model CMS.Models.ViewModels.Preset
@{
    ViewData["Meta"] = new string[3] { "Edit Preset", "MetaKeyword", "MetaDescription" };
}
<!DOCTYPE html>

<html>
<head>
    <title>Edit</title>
</head>
<body>
    <p align="center" style="font-size:50px;margin-top:50px;" class="title">Edit presets</p>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Edit">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input type="hidden" asp-for="presetId" />
                <div class="form-group">
                    <label asp-for="themeId" class="control-label">Theme Name</label>
                    <select asp-for="themeId" class="form-control" asp-items="@ViewBag.namelist "></select>
                    <span asp-validation-for="themeId" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="visitId" class="control-label"></label>
                    <select asp-for="visitId" class="form-control" asp-items="@ViewBag.selectlist "></select>
                    <span asp-validation-for="visitId" class="text-danger"></span>
                </div>
                <input asp-for="dateCreated" class="form-control" style="display: none;" />
                <div class="form-group">
                    <input type="submit" value="Save" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Back to List</a>
    </div>

</body>
</html>
