@model CMS.Models.ViewModels.Preset
@{
    ViewData["Meta"] = new string[3] { "Edit Preset", "MetaKeyword", "MetaDescription" };
    ViewData["Title"] = "Edit Preset";
}
<!DOCTYPE html>

<html>
<head>
    <title>Edit</title>
</head>
<body>
    <br /> <p align="center" style="font-size:50px;" >Edit preset</p>
    <hr />
    <div class="row d-flex justify-content-center">
        <div class="col-md-4">
            <form asp-action="Edit" method="post">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <input asp-for="presetId" hidden="hidden" />
                <input asp-for="dateCreated" hidden="hidden" />
                <div class="form-group">
                    <label asp-for="presetName" class="control-label">presetName</label>
                    <input asp-for="presetName" class="form-control" />
                    <span asp-validation-for="presetName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="themeId" class="control-label"></label>
                    <select asp-for="themeId" class="form-control" asp-items="@ViewBag.namelist "></select>
                    <span asp-validation-for="themeId" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="visitId" class="control-label"></label>
                    <select asp-for="visitId" class="form-control" asp-items="@ViewBag.selectlist "></select>
                    <span asp-validation-for="visitId" class="text-danger"></span>
                </div>
                <div class="form-group d-flex justify-content-center">
                    <input type="submit" value="Save" class="btn btn-info align-self-center" />
                    <a asp-action="Index" class="btn btn-outline-info" style="margin:15px;">Back to List</a>
                </div>
            </form>
        </div>
    </div>
</body>
</html>
