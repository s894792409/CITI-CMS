@model IEnumerable<CMS.Models.ViewModels.Preset>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.dateCreated)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.themeId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.visitId)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.themeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.visitId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.presetId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.presetId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.presetId">Delete</a>
                
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
