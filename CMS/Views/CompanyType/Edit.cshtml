@model CMS.Models.ViewModels.CompanyType

@{
    ViewData["Title"] = "Edit company type";
}

<!DOCTYPE html>

<html>
<head>
    <title>Edit</title>
</head>
<body>
    <p align="center" style="font-size:50px;margin-top:50px;" class="title">Edit company type Menu</p>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="companyTypeId" />
            <div class="form-group">
                <label asp-for="companyType" class="control-label"></label>
                <input asp-for="companyType" class="form-control" />
